{
    "cleanUrls": true,
    "trailingSlash": false,
    "rewrites": [
        {
            "source": "/privacy",
            "destination": "/privacy.html"
        },
        {
            "source": "/terms",
            "destination": "/terms.html"
        },
        {
            "source": "/__/auth/:path*",
            "destination": "https://lovail.firebaseapp.com/__/auth/:path*"
        }
    ]
}