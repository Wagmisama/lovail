{
    "cleanUrls": true,
    "trailingSlash": false,
    "routes": [
        {
            "src": "/privacy",
            "dest": "/privacy.html"
        },
        {
            "src": "/terms",
            "dest": "/terms.html"
        },
        {
            "src": "/__/auth/(.*)",
            "dest": "https://lovail.firebaseapp.com/__/auth/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ]
}